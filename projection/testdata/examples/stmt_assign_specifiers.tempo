struct@(A,B) Pair {
  x: Int@A;
  y: Int@B;
}

struct@(A,B,C) Foo {
  pairs: [Pair@(A,B)];
  z: Int@C;
}

func@(X,Y,Z) main() {
  let foo = Foo@(X,Y,Z) {
    pairs: [],
    z: 1
  };

  foo.pairs = [
    Pair@(X,Y) { x: 1, y: 2 },
    Pair@(X,Y) { x: 3, y: 4 }
  ];
  
  foo.pairs[await Z->X foo.z].x = 10;
}
